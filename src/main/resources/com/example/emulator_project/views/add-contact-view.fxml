<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.geometry.Insets?>
<AnchorPane prefHeight="620.0" prefWidth="360.0" stylesheets="@../styles/add-contact.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.emulator_project.controllers.AddContactController">
   <children>
      <BorderPane prefHeight="620.0" prefWidth="360.0">
          <bottom>
              <HBox fx:id="nav" prefHeight="45.0" prefWidth="360.0" BorderPane.alignment="CENTER">
