                          </children>
                      </Pane>
                      <Pane prefHeight="200.0" prefWidth="200.0">
                          <children>
                              <Button layoutX="1.0" layoutY="-2.0" mnemonicParsing="false" onAction="#goBack" prefHeight="45.0" prefWidth="120.0" styleClass="nav-button" text="&lt;" textFill="WHITE">
