<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="com.example.emulator_project.controllers.tabs.ContactsTabController" minHeight="0.0"
            minWidth="0.0" prefHeight="282.0" prefWidth="367.0">
    <children>
        <VBox layoutX="4.0" prefHeight="586.0" prefWidth="363.0">
            <children>
                <HBox prefHeight="40.0" prefWidth="360.0">
                    <children>
                        <TextField prefHeight="24.0" prefWidth="277.0"/>
                        <Button mnemonicParsing="false" prefHeight="24.0"
                                prefWidth="71.0" text="search"/>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </HBox>
                <ScrollPane fx:id="contactsPane" hbarPolicy="NEVER" prefHeight="501.0"
                            prefWidth="363.0" vbarPolicy="NEVER" VBox.vgrow="NEVER">
                    <content>
                        <VBox fx:id="contactsPaneVBox" prefHeight="489.0"
                              prefWidth="359.0" spacing="5.0">
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0"
                                        top="10.0"/>
                            </padding>
                        </VBox>
                    </content>
                </ScrollPane>
                <Button fx:id="newContactButton" alignment="CENTER"
                        contentDisplay="CENTER" mnemonicParsing="false"
                        onAction="#createNewContact" prefHeight="40.0" prefWidth="343.0"
                        text="Add New Contact +">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
